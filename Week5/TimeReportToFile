import csv

with open("time_report.csv", "w") as f:
    csv_writer = csv.writer(f)
    csv_writer.writerow(["Day", "Arrival", "Departure"])
    csv_writer.writerow(["Monday", "8:00", "16:42"])
    csv_writer.writerow(["Tuesday", "8:15", "16:58"])
    csv_writer.writerow(["Wednesday", "8:16", "16:58"])
    csv_writer.writerow(["Thursday", "8:17", "16:59"])
    csv_writer.writerow(["Friday", "8:18", "16:61"])
